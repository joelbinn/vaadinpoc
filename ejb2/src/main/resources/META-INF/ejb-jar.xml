<!DOCTYPE ejb-jar PUBLIC
        "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
        "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        <session>
            <ejb-name>HelloEJB2</ejb-name>
            <home>
                se.tillvaxtverket.nyps2020.vaadinpoc.ejb2.HelloRemoteHomeEJB2
            </home>
            <remote>se.tillvaxtverket.nyps2020.vaadinpoc.ejb2.HelloRemoteEJB2
            </remote>
            <local-home>
                se.tillvaxtverket.nyps2020.vaadinpoc.ejb2.HelloBeanLocalHomeEJB2
            </local-home>
            <local>se.tillvaxtverket.nyps2020.vaadinpoc.ejb2.HelloLocalEJB2
            </local>
            <ejb-class>se.tillvaxtverket.nyps2020.vaadinpoc.ejb2.HelloBeanEJB2
            </ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>
